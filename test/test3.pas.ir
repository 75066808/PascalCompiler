entry hello_outer_inner1  
    __hello_outer_inner1 = a + b
    return __hello_outer_inner1  
entry hello_outer_inner2  
    begin_args   
    arg aa  
    arg b  
    aa = call hello_outer_inner1 
    k = k + 10
    return   
entry hello_outer  
    k = 0  
    begin_args   
    arg k  
    arg 10  
    call hello_outer_inner2  
    return   
entry hello  
    begin_args   
    call hello_outer  
    begin_args   
    arg k  
    call @sys_writeln  
    return   
